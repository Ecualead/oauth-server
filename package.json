  "name": "auth",
  "version": "3.0.0",
  "description": "ECUALEAD Identity Management Service",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "init-data": "tsc && node dist/scripts/init-data.js",
    "build": "tsc",
    "dev": "concurrently --kill-others \"npm run watch-ts\"",
    "start-dev": "nodemon dist/index.js",
    "watch-ts": "tsc-watch -w -p ./ --onSuccess \"npm run start-dev\"",
    "lint": "tsc --noEmit && eslint '*/**/*.ts' --fix",
    "test": "mocha --require ts-node/register tests/*.ts",
    "coverage": "nyc npm run test",
    "apidoc": "apidoc -c ./ -i ./src/routers -o ./docs/api/"
  },
  "_moduleAliases": {
    "@": "dist/"
  },
  "keywords": [
    "auth",
    "microservice",
    "kyc",
    "security",
    "oauth2"
  ],
  "repository": {
    "type": "git",
